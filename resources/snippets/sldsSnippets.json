{
    "SLDS Modal Dialog": {
        "prefix": "slds.modal",
        "body": [
            "<div style=\"height:640px\">",
            "\t<section role=\"dialog\" tabindex=\"-1\" aria-labelledby=\"modal-heading-01\" aria-modal=\"true\" aria-describedby=\"modal-content-id-1\" class=\"slds-modal slds-fade-in-open\">",
            "\t\t<div class=\"slds-modal__container\">",
            "\t\t\t<header class=\"slds-modal__header\">",
            "\t\t\t\t<button class=\"slds-button slds-button_icon slds-modal__close slds-button_icon-inverse\" title=\"${1:Close}\">",
            "\t\t\t\t\t<svg class=\"slds-button__icon slds-button__icon_large\" aria-hidden=\"true\">",
            "\t\t\t\t\t\t<use xlink:href=\"/assets/icons/utility-sprite/svg/symbols.svg#close\"></use>",
            "\t\t\t\t\t</svg>",
            "\t\t\t\t\t<span class=\"slds-assistive-text\">\"${2:Close}\"</span>",
            "\t\t\t\t</button>",
            "\t\t\t\t<h2 id=\"modal-heading-01\" class=\"slds-text-heading_medium slds-hyphenate\">\"${3:Header Text}\"</h2>",
            "\t\t\t</header>",
            "\t\t\t<div class=\"slds-modal__content slds-p-around_medium\" id=\"modal-content-id-1\">",
            "\t\t\t\t${0:<!-- Put yout modal content here -->}",
            "\t\t\t</div>",
            "\t\t\t<footer class=\"slds-modal__footer\">",
            "\t\t\t\t<button class=\"slds-button slds-button_neutral\">${4:Cancel}</button>",
            "\t\t\t\t<button class=\"slds-button slds-button_brand\">${5:Save}</button>",
            "\t\t\t</footer>",
            "\t\t</div>",
            "\t</section>",
            "\t<div class=\"slds-backdrop slds-backdrop_open\"></div>",
            "</div>"
        ],
        "description": "Create a base Modal Dialog Code Unit with Salesforce Lightning Design System (SLDS) [https://www.lightningdesignsystem.com/components/modals/#Base]"
    },
